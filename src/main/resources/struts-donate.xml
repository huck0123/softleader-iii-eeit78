<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="donatePackage" namespace="/donate" extends="struts-default">

		<action name="donateAction" class="donateAction">
			<result name="success">/META-INF/donate/billInfo.jsp</result>
		</action>

		<action name="billInfoAction" class="billInfoAction">
			<result name="success">/META-INF/donate/infoConfirm.jsp</result>
		</action>
		
				<action name="infoConfirmAction" class="infoConfirmAction">
			<result name="success">/META-INF/donate/donateSuccess.jsp</result>
		</action>

		<action name="*">
			<result>/META-INF/donate/{1}.jsp</result>
		</action>

	</package>




</struts>
