<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="donatePackage" namespace="/donate" extends="struts-default">

		<action name="donateAction" class="donateAction">
			<result name="success" type="redirect">/donate/donateSuccess</result>
			<result name="fail" type="redirect">/donate/donate</result>
		</action>


		<action name="*">
			<result>/META-INF/donate/{1}.jsp</result>
		</action>

	</package>




</struts>
