<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="raiserPackage" namespace="/raiser" extends="count">
		<default-interceptor-ref name="myCountInterceptor"></default-interceptor-ref>

		<action name="raiserAction" class="tw.org.iiiedu.thegivers.web.RaiserAction">
			<result name="insert" type="redirect">/META-INF/raiser/raiserCheck.jsp</result>
			<result name="update" type="redirect">/META-INF/raiser/raiserCheck.jsp</result>
			<result name="select" type="redirect">/META-INF/raiser/raiserCheck.jsp</result>
			<result name="error" type="redirect">/META-INF/raiser/raiserRegister.jsp
			</result>
			<result name="success" type="redirect">/META-INF/login/login.jsp</result>
		</action>

		<action name="*">
			<result>/META-INF/raiser/{1}.jsp</result>
		</action>
	</package>

</struts>
