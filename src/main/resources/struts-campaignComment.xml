<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="campaignCommentPackage" namespace="/campaignComment" extends="user-defined">
		<action name="actNewComment" class="campaignCommentAction">
			<result name="leaveNewComment" type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<action name="actAllComment" class="campaignCommentAction">
			<result name="fetchAllComment" type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<action name="actReplyComment" class="campaignCommentAction">
			<result name="findReplyComment" type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
	</package>
</struts>