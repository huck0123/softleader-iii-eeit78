<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

<!-- 	<constant name="struts.objectFactory" value="spring" />把Action交給spring -->
<!-- 	<constant name="struts.devMode" value="true" /> -->
<!-- 	<constant name="struts.ui.theme" value="simple" /> -->
<!-- 	<constant name="struts.enable.DynamicMethodInvocation" value="true" /> -->
<!-- 	<constant name="struts.custom.i18n.resources" value="locale/global" /> -->


	<package name="giverPackage" namespace="/giver" extends="count">
		<default-interceptor-ref name="myCountInterceptor"></default-interceptor-ref>

		<action name="giverAction" class="giverAction">
			<result name="insert" type="redirect">/giver/registerSuccess</result>
			<result name="update" type="">/index.jsp</result>
			<result name="success" type="">/index.jsp</result>
			<result name="fail" type="">/index.jsp</result>
			<result name="input" type="">/META-INF/giver/giverRegister.jsp</result>
		</action>
		
		<action name="giverSelect" class="giverAction">
			<result type="stream" name="select">
				<param name="contentType">text/html</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>

		<action name="*">
			<result type="">/META-INF/giver/{1}.jsp</result> 
		</action>
		
	</package>




</struts>
